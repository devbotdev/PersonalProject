<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="me.ib.PersonalProject.control.MainController">
   <Pane fx:id="bgPane" prefHeight="200.0" prefWidth="200.0"/>
   <BorderPane fx:id="mainPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="540.0" prefWidth="960.0">
      <center>
         <VBox fx:id="vBox" alignment="CENTER" prefHeight="540.0" prefWidth="960.0" spacing="4.0"
               stylesheets="@stylesheet.css" BorderPane.alignment="CENTER">
            <Label id="label-title" fx:id="titleLabel" alignment="CENTER" stylesheets="@stylesheet.css"
                   text="Space Simulator" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="72.0"/>
               </font>
            </Label>
            <Button id="menuButton" fx:id="launchButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="350.0"
                    text="Launch">
               <font>
                  <Font name="Kinetika Ultra" size="12.0"/>
               </font>
               <VBox.margin>
                  <Insets top="50.0"/>
               </VBox.margin>
            </Button>
            <Button id="menuButton" fx:id="optionsButton" mnemonicParsing="false" prefHeight="80.0" prefWidth="350.0"
                    text="Options">
               <VBox.margin>
                  <Insets/>
               </VBox.margin>
            </Button>
            <Button id="menuButton" fx:id="exitButton" alignment="CENTER" mnemonicParsing="false" prefHeight="80.0"
                    prefWidth="350.0" text="Exit"/>
         </VBox>
      </center>
   </BorderPane>
</StackPane>
